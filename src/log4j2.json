{
	"configuration": {
		"appenders": {
			"Console": {
				"name": "Console",
				"target": "SYSTEM_OUT",
				"PatternLayout": {
					"pattern": "%date %thread %logger %message\n"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "debug",
				"appender-ref": {
					"ref": "Console"
				}
			}
		}
	}
}