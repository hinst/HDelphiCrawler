{
	"configuration": {
		"appenders": {
			"Console": {
				"name": "Console",
				"target": "SYSTEM_OUT",
				"PatternLayout": {
					"pattern": "%date{yyyyMMdd_HH:mm:ss.SSS}_%thread_%logger{1.}: %message\n"
				}
			}
		},
		"loggers": {
			"root": {
				"level": "debug",
				"appender-ref": {
					"ref": "Console"
				}
			}
		}
	}
}